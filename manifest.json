{

  "manifest_version": 2,
  "name": "JS_Rnnuer",
  "version": "1.0",

  "description": "Runs JS files on the browser",

  // "icons": {
  //   "48": "icons/border-48.png"
  // },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Runner.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action":{
    "default_icon": "icon.png"
  }

}